{
  "name": "rescript-decode",
  "displayName": "Rescript Decode",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "title": "Rescript Decode",
        "command": "rescript-decode.enableDecodeButton",
        "category": ""
      }
    ],
    "configuration": {
      "properties": {
        "rescript-decode.enableDecodeButton": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "re:build": "rescript",
    "re:clean": "rescript clean",
    "re:start": "rescript build -w"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.81.0",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "rescript": "^10.1.4"
  }
}
